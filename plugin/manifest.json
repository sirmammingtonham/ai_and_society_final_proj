{
  "manifest_version": 2,

  "name": "FakeBlock",
  "description": "Detect and block DeepFakes and AI generated articles.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "fakeblock.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}